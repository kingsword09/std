{
  "name": "@kingsword/nodekit",
  "version": "0.0.9",
  "description": "Node utility standard library for individual use",
  "license": "Apache-2.0 or MIT",
  "exports": {
    ".": "./mod.ts",
    "./fs": "./src/fs.ts",
    "./json": "./src/json.ts",
    "./path": "./src/path.ts",
    "./which": "./src/which.ts"
  },
  "imports": {
    "@std/jsonc": "jsr:@std/jsonc@^1.0.2",
    "quansync": "npm:quansync@^0.2.10"
  },
  "exclude": [
    "dist/"
  ],
  "publish": {
    "include": [
      "src/*.ts",
      "mod.ts",
      "README.md"
    ],
    "exclude": [
      "build.ts"
    ]
  }
}